<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.appdev.shan.childtracker.Main2Activity">

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="50dp"
        android:layout_marginStart="50dp"
        android:background="@drawable/phonecall"
        android:id="@+id/CallF"
        />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="150dp"
        android:layout_marginStart="50dp"
        android:background="@drawable/phonemessage"
        android:id="@+id/MessageLogF"
        />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="250dp"
        android:layout_marginStart="50dp"
        android:background="@drawable/appuse"
        android:id="@+id/AppLogF"
        />

</RelativeLayout>
