        android:text="Message Log"
        android:textColor="#7b2f2f"
        android:textSize="20sp"
        android:layout_marginLeft="7dp" />

    <ImageButton
        android:id="@+id/btnCall"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="7dp"
        android:layout_below="@+id/btnMsg"
        android:layout_marginTop="10dp"
        android:background="@drawable/phonecall"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/msgCall"
        android:layout_marginStart="7dp"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/btnMsg"
        android:layout_toRightOf="@+id/btnCall"
        android:text="  Call Log"
        android:textColor="#7b2f2f"
        android:textSize="20sp"/>

    <ImageButton
        android:id="@+id/btnApp"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="10dp"
        android:layout_below="@id/btnCall"
        android:layout_marginLeft="50dp"
        android:background="@drawable/appuse"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/msgApp"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/btnCall"
        android:text="  App Log"
        android:layout_toRightOf="@+id/btnApp"
        android:layout_marginLeft="7dp"
        android:textColor="#7b2f2f"
        android:textSize="20sp"/>

    <ImageButton
        android:id="@+id/btnGPS"
        android:background="@drawable/map"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@+id/btnApp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="50dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnApp"
        android:text="  Location Log"
        android:layout_toRightOf="@+id/btnGPS"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="40dp"
        android:textColor="#7b2f2f"
        android:textSize="20sp"/>

    <ImageButton
        android:id="@+id/btnAppWeekly"
